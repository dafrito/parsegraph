  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.1",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.8.3",
    "raw-loader": "^4.0.2",
    "source-map-loader": "^4.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "all": "yarn lint && yarn test:cov && yarn build && yarn doc",
    "build": "webpack --mode development --config config/webpack.config.js",
    "build:watch": "yarn build --watch",
    "build:prod": "NODE_ENV=production webpack --mode production --config config/webpack.prod.js",
    "build:prod:watch": "NODE_ENV=production yarn build:prod --watch",
    "test": "jest --env=jsdom --no-cache",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage --coverageDirectory=dist/coverage",
    "prettier": "prettier --write src",
    "lint": "yarn prettier && npx eslint --fix 'src/**'",
    "doc": "typedoc --tsconfig config/tsconfig.browser.json src/index.ts --out ./dist/docs",
    "clean": "./scripts/clean",
    "tar": "./scripts/tar-dev parsegraph",
    "tar:prod": "./scripts/tar-prod parsegraph",
    "deploy": "./scripts/deploy",
    "deploy:prod": "./scripts/deploy-prod"
  }
}
