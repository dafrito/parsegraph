{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "../dist-prod/src",
    "noImplicitAny": true,
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "sourceMap":false,
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true
  ,
  "include": [
    "../src"
  ],
  "exclude": [
    "../node_modules"
  ]
}
